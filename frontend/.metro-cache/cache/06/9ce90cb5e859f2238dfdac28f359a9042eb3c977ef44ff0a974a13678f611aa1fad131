 }\n    };\n    const handleAttachFile = async taskId => {\n      try {\n        const result = await DocumentPicker.getDocumentAsync({\n          type: '*/*',\n          copyToCacheDirectory: true\n        });\n        if (!result.canceled && result.assets[0]) {\n          const file = result.assets[0];\n          const task = tasks.find(t => t.id === taskId);\n          const currentAttachments = task?.attachments || [];\n          const newAttachment = {\n            type: 'file',\n            name: file.name,\n            uri: file.uri,\n            size: file.size,\n            mimeType: file.mimeType\n          };\n          const updatedAttachments = [...currentAttachments, newAttachment];\n          const response = await fetch(`${API_BASE_URL}/api/tasks/${taskId}`, {\n            method: 'PUT',\n            headers: {\n              'Authorization': `Bearer ${token}`,\n              'Content-Type': 'application/json'\n            },\n            body: JSON.stringify({\n              attachments: updatedAttachments\n            })\n          });\n          if (response.ok) {\n            fetchLoopData();\n            (0, _reactNativeFlashMessage.showMessage)({\n              message: 'File attached successfully!',\n              type: 'success'\n            });\n          }\n        }\n      } catch (error) {\n        (0, _reactNativeFlashMessage.showMessage)({\n          message: 'Failed to attach file',\n          type: 'danger'\n        });\n      }\n    };\n    const handleAttachImage = async taskId => {\n      console.log('ðŸ”¥ Camera icon clicked! Task ID:', taskId);\n\n      // Show immediate feedback that the camera button was clicked\n      (0, _reactNativeFlashMessage.showMessage)({\n        message: 'Camera button clicked! Opening options...',\n        type: 'info'\n      });\n      try {\n        // For web development, just open image library directly\n        // On mobile, this will show the proper Alert\n        if (Platform.default.OS === 'web') {\n          console.log('ðŸ“· Web platform detected, opening image library...');\n          openImageLibrary(taskId);\n        } else {\n          console.log('ðŸ“± Mobile platform detected, showing camera options...');\n          // Show options for camera or photo library on mobile\n          Alert.default.alert('Add Photo', 'Choose an option:', [{\n            text: 'Cancel',\n            style: 'cancel'\n          }, {\n            text: 'Take Photo',\n            onPress: () => openCamera(taskId)\n          }, {\n            text: 'Choose from Library',\n            onPress: () => openImageLibrary(taskId)\n          }]);\n        }\n      } catch (error) {\n        console.log('âŒ Error in handleAttachImage:', error);\n        (0, _reactNativeFlashMessage.showMessage)({\n          message: `Error accessing camera: ${error.message}`,\n          type: 'danger'\n        });\n      }\n    };\n    const openCamera = async taskId => {\n      try {\n        // Request camera permissions\n        const {\n          status\n        } = await ImagePicker.requestCameraPermissionsAsync();\n        if (status !== 'granted') {\n          Alert.default.alert('Permission needed', 'Camera access is required to take photos.');\n          return;\n        }\n        const result = await ImagePicker.launchCameraAsync({\n          mediaTypes: ImagePicker.MediaTypeOptions.Images,\n          allowsEditing: true,\n          aspect: [4, 3],\n          quality: 0.7,\n          base64: true\n        });\n        if (!result.canceled && result.assets[0]) {\n          await processImageResult(result.assets[0], taskId);\n        }\n      } catch (error) {\n        console.log('Error opening camera:', error);\n        (0, _reactNativeFlashMessage.showMessage)({\n          message: 'Error opening camera',\n          type: 'danger'\n        });\n      }\n    };\n    const openImageLibrary = async taskId => {\n      console.log('ðŸ“± Opening image library for task:', taskId);\n      try {\